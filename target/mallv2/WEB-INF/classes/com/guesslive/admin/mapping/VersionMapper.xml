<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.guesslive.admin.dao.VersionMapper">
	 <insert id="insertVersion" parameterType="ProjectVersion" useGeneratedKeys="true" keyProperty="id">
	    insert into t_project_version(ptype,versionname,versioncode,updatedesc,fileurl,createtime) values (#{ptype},#{versionname},#{versioncode},#{updatedesc},#{fileurl},now())
     </insert>
     
     <select id="getVersion" resultType="ProjectVersion">
     	select ptype,versionname,updatedesc,fileurl,date_format(createtime,'%y-%m-%d %H:%I:%S') createtime from t_project_version where ptype = #{ptype} order by createtime DESC
     </select>
</mapper>