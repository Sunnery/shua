<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.guesslive.admin.dao.ActiveNeworderMapper">

	<insert id="insertActiveNeworder" useGeneratedKeys="true"
		keyProperty="id">
		insert into t_active_neworder
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="goodsid != null">
				goodsid,
			</if>
			<if test="benefit != null">
				benefit
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="goodsid != null">
				#{goodsid},
			</if>
			<if test="benefit != null">
				#{benefit}
			</if>
		</trim>
	</insert>

	<select id="selectAll" resultType="ActiveNeworder">
		select
		an.* , g.goodsname ,g.iconurl 
		from t_active_neworder an LEFT JOIN t_goods g ON
		an.goodsid=g.goodsid
		order by id DESC
		limit #{start},#{rows}
	</select>

	<select id="selectTotal" resultType="int">
		select count(*) from t_active_neworder an LEFT JOIN t_goods g ON
		an.goodsid=g.goodsid
	</select>

	<delete id="deleteById" parameterType="int">
		delete from
		t_active_neworder where id=#{id}
	</delete>

</mapper>